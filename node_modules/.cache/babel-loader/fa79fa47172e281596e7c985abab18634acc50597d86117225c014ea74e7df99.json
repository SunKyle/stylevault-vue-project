lative z-10\">\n            <div class=\"text-center\">\n              <p class=\"text-sm text-white/80\">湿度</p>\n              <p class=\"font-medium mt-1\">{{ weather.humidity }}</p>\n            </div>\n            <div class=\"text-center\">\n              <p class=\"text-sm text-white/80\">紫外线</p>\n              <p class=\"font-medium mt-1\">{{ weather.uv }}</p>\n            </div>\n            <div class=\"text-center\">\n              <p class=\"text-sm text-white/80\">风速</p>\n              <p class=\"font-medium mt-1\">{{ weather.wind }}</p>\n            </div>\n            <div class=\"text-center\">\n              <p class=\"text-sm text-white/80\">气压</p>\n              <p class=\"font-medium mt-1\">{{ weather.pressure }}</p>\n            </div>\n            <div class=\"text-center hidden md:block\">\n              <p class=\"text-sm text-white/80\">日出日落</p>\n              <p class=\"font-medium mt-1\">{{ weather.sunrise }}/{{ weather.sunset }}</p>\n            </div>\n          </div>\n        </div>\n      </transition>\n      <!-- 穿搭推荐 -->\n      <h3 class=\"text-xl md:text-2xl font-bold mb-6\">为您推荐</h3>\n      <div class=\"grid grid-cols-1 md:grid-cols-2 gap-6 mb-12\">\n        <transition-group name=\"fade\" tag=\"div\">\n          <div v-for=\"rec in recommendations\" :key=\"rec.title\" class=\"bg-white rounded-2xl p-6 shadow-soft hover:shadow-medium transition-all duration-300\">\n            <div class=\"flex items-center justify-between mb-5\">\n              <h4 class=\"font-bold text-lg\">{{ rec.title }}</h4>\n              <span :class=\"rec.tagClass\">{{ rec.tag }}</span>\n            </div>\n            <div class=\"flex flex-wrap gap-4 mb-5\">\n              <div v-for=\"(img, idx) in rec.images\" :key=\"idx\" class=\"w-20 h-20 rounded-xl overflow-hidden shadow-soft\">\n                <img :src=\"img\" class=\"w-full h-full object-cover\" />\n              </div>\n            </div>\n            <p class=\"text-neutral-600 mb-5\">{{ rec.desc }}</p>\n            <div class=\"flex justify-between items-center\">\n              <button class=\"text-primary font-medium group flex items-center\">\n                <span>查看详情</span>\n                <font-awesome-icon :icon=\"['fas', 'arrow-right']\" class=\"ml-1 text-sm group-hover:translate-x-1 transition-transform\" />\n              </button>\n              <button class=\"bg-primary/10 text-primary font-medium px-5 py-2.5 rounded-xl hover:bg-primary/20 transition-colors\">\n                保存搭配\n              </button>\n            </div>\n          </div>\n        </transition-group>\n      </div>\n      <!-- 未来几天预报 -->\n      <h3 class=\"text-xl md:text-2xl font-bold mb-6\">未来几天穿搭预览</h3>\n      <div class=\"grid grid-cols-2 md:grid-cols-4 gap-5\">\n        <transition-group name=\"fade\" tag=\"div\">\n          <div v-for=\"day in forecast\" :key=\"day.week\" class=\"bg-white rounded-2xl p-5 shadow-soft hover:shadow-medium transition-all duration-300\">\n            <p class=\"font-medium mb-3\">{{ day.week }}</p>\n            <div class=\"flex items-center justify-between mb-4\">\n              <font-awesome-icon :icon=\"day.icon\" :class=\"day.iconClass\" />\n              <span class=\"font-bold text-lg\">{{ day.temp }}°C</span>\n            </div>\n            <div class=\"flex gap-2 mb-4 overflow-x-auto pb-2 scrollbar-hide\">\n              <div v-for=\"(img, idx) in day.images\" :key=\"idx\" class=\"w-14 h-14 rounded-lg overflow-hidden flex-shrink-0 shadow-soft\">\n                <img :src=\"img\" class=\"w-full h-full object-cover\" />\n              </div>\n            </div>\n            <button class=\"w-full bg-neutral-50 hover:bg-neutral-100 text-neutral-800 font-medium py-2 rounded-xl transition-colors\">\n              查看推荐\n            </button>\n          </div>\n        </transition-group>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script setup>\nconst weather = {\n  city: '北京市',\n  date: '星期三, 6月15日',\n  temp: 28,\n  