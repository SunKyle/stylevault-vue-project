{"ast":null,"code":"import { ref, reactive, computed } from 'vue';\n\n// Props定义\n\nexport default {\n  __name: 'OutfitCard',\n  props: {\n    outfit: {\n      type: Object,\n      required: true\n    }\n  },\n  emits: ['load-outfit', 'delete-outfit', 'edit-outfit'],\n  setup: function setup(__props, _ref) {\n    var __expose = _ref.expose,\n      __emit = _ref.emit;\n    __expose();\n    var props = __props;\n\n    // 事件定义\n    var emit = __emit;\n\n    // 状态管理\n    var expanded = ref(false);\n\n    // 场景选项映射\n    var sceneOptions = [{\n      value: 'daily',\n      label: '日常'\n    }, {\n      value: 'work',\n      label: '工作'\n    }, {\n      value: 'party',\n      label: '聚会'\n    }, {\n      value: 'date',\n      label: '约会'\n    }, {\n      value: 'travel',\n      label: '旅行'\n    }, {\n      value: 'sports',\n      label: '运动'\n    }];\n\n    // 季节选项映射\n    var seasonOptions = [{\n      value: 'spring',\n      label: '春季'\n    }, {\n      value: 'summer',\n      label: '夏季'\n    }, {\n      value: 'autumn',\n      label: '秋季'\n    }, {\n      value: 'winter',\n      label: '冬季'\n    }];\n\n    // 风格选项映射\n    var styleOptions = [{\n      value: 'casual',\n      label: '休闲'\n    }, {\n      value: 'formal',\n      label: '正式'\n    }, {\n      value: 'business',\n      label: '商务'\n    }, {\n      value: 'street',\n      label: '街头'\n    }, {\n      value: 'vintage',\n      label: '复古'\n    }, {\n      value: 'minimalist',\n      label: '极简'\n    }];\n\n    // 获取场景标签\n    function getSceneLabel(value) {\n      var scene = sceneOptions.find(function (option) {\n        return option.value === value;\n      });\n      return scene ? scene.label : value;\n    }\n\n    // 获取季节标签\n    function getSeasonLabel(value) {\n      var season = seasonOptions.find(function (option) {\n        return option.value === value;\n      });\n      return season ? season.label : value;\n    }\n\n    // 获取风格标签\n    function getStyleLabel(value) {\n      var style = styleOptions.find(function (option) {\n        return option.value === value;\n      });\n      return style ? style.label : value;\n    }\n\n    // 选择场景\n    function selectScene(value) {\n      var index = editOutfit.scenes.indexOf(value);\n      if (index > -1) {\n        editOutfit.scenes.splice(index, 1); // 如果已选中，则取消选择\n      } else {\n        editOutfit.scenes.push(value); // 添加到选择数组\n      }\n    }\n\n    // 选择季节\n    function selectSeason(value) {\n      var index = editOutfit.seasons.indexOf(value);\n      if (index > -1) {\n        editOutfit.seasons.splice(index, 1); // 如果已选中，则取消选择\n      } else {\n        editOutfit.seasons.push(value); // 添加到选择数组\n      }\n    }\n\n    // 选择风格\n    function selectStyle(value) {\n      var index = editOutfit.styles.indexOf(value);\n      if (index > -1) {\n        editOutfit.styles.splice(index, 1); // 如果已选中，则取消选择\n      } else {\n        editOutfit.styles.push(value); // 添加到选择数组\n      }\n    }\n    var hoveredIndex = ref(-1);\n\n    // 切换展开状态\n    function toggleExpand() {\n      expanded.value = !expanded.value;\n    }\n\n    // 获取堆叠样式\n    function getItemStyle(index) {\n      var _props$outfit;\n      var totalItems = Math.min(((_props$outfit = props.outfit) === null || _props$outfit === void 0 || (_props$outfit = _props$outfit.items) === null || _props$outfit === void 0 ? void 0 : _props$outfit.length) || 0, 4);\n      var maxRotation = 15; // 最大旋转角度\n      var maxOffset = 15; // 最大偏移量\n\n      // 根据索引计算位置和旋转\n      var rotation = (index - (totalItems - 1) / 2) * (maxRotation / totalItems);\n      var offset = (index - (totalItems - 1) / 2) * (maxOffset / totalItems);\n      return {\n        transform: \"translateX(\".concat(offset, \"px) rotate(\").concat(rotation, \"deg)\"),\n        zIndex: 10 + index,\n        opacity: hoveredIndex.value === -1 || hoveredIndex.value === index ? 1 : 0.7\n      };\n    }\n\n    // 重置堆叠效果\n    function resetStack() {\n      hoveredIndex.value = -1;\n    }\n\n    // 图片预览\n    var previewImage = ref({\n      show: false,\n      url: '',\n      name: '',\n      type: ''\n    });\n\n    // 打开图片预览\n    function openImagePreview(item) {\n      previewImage.value = {\n        show: true,\n        url: item.img,\n        name: item.name,\n        type: item.type\n      };\n    }\n\n    // 关闭图片预览\n    function closeImagePreview() {\n      previewImage.value.show = false;\n    }\n\n    // 编辑状态\n    var isEditing = ref(false);\n    var editOutfit = reactive({\n      id: '',\n      name: '',\n      scenes: [],\n      // 改为数组，支持多选\n      seasons: [],\n      // 改为数组，支持多选\n      styles: [] // 改为数组，支持多选\n    });\n\n    // 切换编辑模式\n    function toggleEditMode() {\n      if (isEditing.value) {\n        cancelEdit();\n      } else {\n        // 进入编辑模式，初始化编辑数据\n        editOutfit.id = props.outfit.id;\n        editOutfit.name = props.outfit.name;\n        // 将字符串转换为数组，支持多选\n        editOutfit.scenes = props.outfit.scene ? [props.outfit.scene] : [];\n        editOutfit.seasons = props.outfit.season ? [props.outfit.season] : [];\n        editOutfit.styles = props.outfit.style ? [props.outfit.style] : [];\n        isEditing.value = true;\n      }\n    }\n\n    // 保存编辑\n    function saveEdit() {\n      emit('edit-outfit', {\n        id: editOutfit.id,\n        name: editOutfit.name,\n        // 将数组转换为字符串，兼容后端\n        scene: editOutfit.scenes.length > 0 ? editOutfit.scenes.join(',') : '',\n        season: editOutfit.seasons.length > 0 ? editOutfit.seasons[0] : '',\n        style: editOutfit.styles.length > 0 ? editOutfit.styles[0] : ''\n      });\n      isEditing.value = false;\n    }\n\n    // 取消编辑\n    function cancelEdit() {\n      isEditing.value = false;\n    }\n    var __returned__ = {\n      props: props,\n      emit: emit,\n      expanded: expanded,\n      sceneOptions: sceneOptions,\n      seasonOptions: seasonOptions,\n      styleOptions: styleOptions,\n      getSceneLabel: getSceneLabel,\n      getSeasonLabel: getSeasonLabel,\n      getStyleLabel: getStyleLabel,\n      selectScene: selectScene,\n      selectSeason: selectSeason,\n      selectStyle: selectStyle,\n      hoveredIndex: hoveredIndex,\n      toggleExpand: toggleExpand,\n      getItemStyle: getItemStyle,\n      resetStack: resetStack,\n      previewImage: previewImage,\n      openImagePreview: openImagePreview,\n      closeImagePreview: closeImagePreview,\n      isEditing: isEditing,\n      editOutfit: editOutfit,\n      toggleEditMode: toggleEditMode,\n      saveEdit: saveEdit,\n      cancelEdit: cancelEdit,\n      ref: ref,\n      reactive: reactive,\n      computed: computed\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","computed","props","__props","emit","__emit","expanded","sceneOptions","value","label","seasonOptions","styleOptions","getSceneLabel","scene","find","option","getSeasonLabel","season","getStyleLabel","style","selectScene","index","editOutfit","scenes","indexOf","splice","push","selectSeason","seasons","selectStyle","styles","hoveredIndex","toggleExpand","getItemStyle","_props$outfit","totalItems","Math","min","outfit","items","length","maxRotation","maxOffset","rotation","offset","transform","concat","zIndex","opacity","resetStack","previewImage","show","url","name","type","openImagePreview","item","img","closeImagePreview","isEditing","id","toggleEditMode","cancelEdit","saveEdit","join"],"sources":["/Users/sunxiaokai/Desktop/stylevault-vue-project/src/components/features/inspiration/OutfitCard.vue"],"sourcesContent":["<template>\n  <div class=\"bg-gradient-to-br from-indigo-50 via-white to-purple-50 rounded-2xl overflow-hidden shadow-lg hover:shadow-2xl transition-all duration-500 group border border-indigo-100/80 flex flex-col transform hover:-translate-y-1.5\">\n    <!-- 搭配预览 - 优化后的卡片顶部 -->\n    <div class=\"relative overflow-hidden\">\n      <!-- 卡片顶部渐变背景 -->\n      <div class=\"absolute inset-0 bg-gradient-to-br from-indigo-500/15 via-purple-500/10 to-white/80 transform transition-transform duration-700 group-hover:scale-110\"></div>\n\n      <!-- 装饰元素 - 增强卡片深度感 -->\n      <div class=\"absolute -top-12 -right-12 w-28 h-28 bg-gradient-to-r from-indigo-400/20 to-purple-400/20 rounded-full blur-2xl opacity-70 animate-pulse-slow\"></div>\n      <div class=\"absolute -bottom-12 -left-12 w-28 h-28 bg-gradient-to-r from-purple-400/20 to-indigo-400/20 rounded-full blur-2xl opacity-70 animate-pulse-slow animation-delay-1000\"></div>\n      <div class=\"absolute top-1/4 left-1/4 w-4 h-4 rounded-full bg-indigo-400/30 animate-ping\"></div>\n      <div class=\"absolute bottom-1/3 right-1/3 w-3 h-3 rounded-full bg-purple-400/30 animate-ping animation-delay-700\"></div>\n\n      <!-- 卡片内容 -->\n      <div class=\"p-5 h-56 flex flex-col relative z-10\">\n        <!-- 顶部信息区 -->\n        <div class=\"flex justify-between items-start mb-3\">\n          <div>\n            <h4 class=\"font-bold text-indigo-900 text-lg truncate pr-2 group-hover:text-indigo-600 transition-colors\">\n              {{ outfit.name }}\n            </h4>\n            <div v-if=\"outfit?.scene\" class=\"flex items-center mt-1.5\">\n              <div class=\"w-5 h-5 rounded-full bg-gradient-to-r from-indigo-500/10 to-purple-500/10 flex items-center justify-center mr-1.5 shadow-sm\">\n                <font-awesome-icon icon=\"map-marker-alt\" class=\"text-indigo-600 text-xs\" />\n              </div>\n              <span class=\"text-xs text-indigo-600 font-medium bg-white/70 backdrop-blur-sm px-2 py-1 rounded-full border border-indigo-100/50\">\n                {{ getSceneLabel(outfit?.scene) || '' }}\n              </span>\n            </div>\n\n            <!-- 季节信息 -->\n            <div v-if=\"outfit?.season\" class=\"flex items-center mt-1.5\">\n              <div class=\"w-5 h-5 rounded-full bg-gradient-to-r from-green-500/10 to-emerald-500/10 flex items-center justify-center mr-1.5 shadow-sm\">\n                <font-awesome-icon icon=\"leaf\" class=\"text-green-600 text-xs\" />\n              </div>\n              <span class=\"text-xs text-green-600 font-medium bg-white/70 backdrop-blur-sm px-2 py-1 rounded-full border border-green-100/50\">\n                {{ getSeasonLabel(outfit.season) }}\n              </span>\n            </div>\n\n            <!-- 风格信息 -->\n            <div v-if=\"outfit?.style\" class=\"flex items-center mt-1.5\">\n              <div class=\"w-5 h-5 rounded-full bg-gradient-to-r from-purple-500/10 to-pink-500/10 flex items-center justify-center mr-1.5 shadow-sm\">\n                <font-awesome-icon icon=\"palette\" class=\"text-purple-600 text-xs\" />\n              </div>\n              <span class=\"text-xs text-purple-600 font-medium bg-white/70 backdrop-blur-sm px-2 py-1 rounded-full border border-purple-100/50\">\n                {{ getStyleLabel(outfit.style) }}\n              </span>\n            </div>\n          </div>\n\n        </div>\n\n        <!-- 衣物预览区 - 堆叠效果 -->\n        <div class=\"flex-1 flex items-center justify-center relative\" @mouseleave=\"resetStack\" style=\"height: 220px;\">\n          <div v-if=\"(outfit?.items?.length || 0) === 0\" class=\"text-center text-indigo-400\">\n            <div class=\"w-16 h-16 rounded-2xl bg-gradient-to-br from-indigo-100/80 to-purple-100/50 backdrop-blur-sm flex items-center justify-center mx-auto mb-3 shadow-lg border border-indigo-100/50\">\n              <font-awesome-icon icon=\"tshirt\" class=\"text-indigo-500 text-2xl\" />\n            </div>\n            <p class=\"text-sm text-indigo-500\">暂无衣物</p>\n          </div>\n          <div v-else class=\"relative w-full h-full overflow-visible\">\n            <!-- 衣物图片堆叠效果 - 重新设计 -->\n            <div class=\"relative w-full h-full flex items-center justify-center\">\n              <template v-for=\"(item, idx) in (outfit?.items || []).slice(0, 4)\" :key=\"idx\">\n              <div\n                v-if=\"item.img\"\n                  class=\"absolute w-28 h-36 bg-gradient-to-br from-white to-indigo-50/50 rounded-xl shadow-lg overflow-hidden transition-all duration-500 ease-out cursor-pointer border-2 border-white/80 backdrop-blur-sm\"\n                  :class=\"{ 'ring-2 ring-indigo-400/30 ring-offset-1': hoveredIndex === idx }\"\n                  :style=\"getItemStyle(idx)\"\n                  @mouseenter=\"hoveredIndex = idx\"\n                  @click=\"openImagePreview(item)\"\n                >\n                  <img :src=\"item.img\" :alt=\"item.name\" class=\"w-full h-full object-cover\" loading=\"lazy\" />\n                  <div class=\"absolute bottom-0 left-0 right-0 bg-gradient-to-t from-black/70 to-transparent p-2\">\n                    <p class=\"text-white text-xs font-medium truncate\">{{ item.name }}</p>\n                    <p class=\"text-white/80 text-xs truncate\">{{ item.type }}</p>\n                  </div>\n                </div>\n              </template>\n            </div>\n\n            <!-- 更多衣物指示器 -->\n            <div v-if=\"(outfit?.items?.length || 0) > 4\" class=\"absolute bottom-2 right-2 w-8 h-8 rounded-full bg-indigo-500 text-white flex items-center justify-center text-xs font-bold shadow-lg z-10\">\n              +{{ (outfit?.items?.length || 0) - 4 }}\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 优化的搭配信息 -->\n    <div class=\"p-5 flex-shrink-0 flex flex-col bg-gradient-to-br from-indigo-50/80 via-white/90 to-purple-50/80 backdrop-blur-sm\">\n      <!-- 衣物数量统计 -->\n      <div class=\"mb-4 flex items-center justify-between\">\n        <span class=\"text-sm font-medium text-indigo-700 flex items-center bg-white/70 backdrop-blur-sm px-3 py-1.5 rounded-full shadow-sm border border-indigo-100/50\">\n          <div class=\"w-5 h-5 rounded-full bg-gradient-to-r from-indigo-500/10 to-purple-500/10 flex items-center justify-center mr-2 shadow-sm\">\n            <font-awesome-icon icon=\"layer-group\" class=\"text-indigo-600 text-xs\" />\n          </div>\n          {{ outfit?.items?.length || 0 }}件衣物\n        </span>\n        <!-- 操作按钮 -->\n        <div class=\"flex gap-2\">\n          <button @click=\"toggleEditMode\"\n                  class=\"w-9 h-9 rounded-full bg-white/80 backdrop-blur-sm flex items-center justify-center text-indigo-500 hover:bg-indigo-500 hover:text-white transition-all duration-300 shadow-md hover:shadow-lg border border-indigo-100/50\"\n                  title=\"编辑搭配\">\n            <font-awesome-icon icon=\"edit\" class=\"text-xs\" />\n          </button>\n          <button @click=\"$emit('load-outfit', outfit)\"\n                  class=\"w-9 h-9 rounded-full bg-white/80 backdrop-blur-sm flex items-center justify-center text-indigo-600 hover:bg-indigo-600 hover:text-white transition-all duration-300 shadow-md hover:shadow-lg border border-indigo-100/50\"\n                  title=\"复制搭配\">\n            <font-awesome-icon icon=\"copy\" class=\"text-xs\" />\n          </button>\n          <button @click=\"$emit('delete-outfit', outfit.id)\"\n                  class=\"w-9 h-9 rounded-full bg-white/80 backdrop-blur-sm flex items-center justify-center text-red-500 hover:bg-red-500 hover:text-white transition-all duration-300 shadow-md hover:shadow-lg border border-indigo-100/50\"\n                  title=\"删除搭配\">\n            <font-awesome-icon icon=\"trash\" class=\"text-xs\" />\n          </button>\n        </div>\n      </div>\n\n\n\n      <!-- 主要操作按钮 -->\n      <button v-if=\"!isEditing\" @click=\"$emit('load-outfit', outfit)\"\n              class=\"w-full bg-gradient-to-r from-primary to-secondary hover:from-primary/90 hover:to-secondary/90 text-white font-medium py-2.5 rounded-xl transition-all duration-300 flex items-center justify-center shadow-md hover:shadow-lg transform hover:-translate-y-1 active:translate-y-0 mt-auto\">\n        <font-awesome-icon :icon=\"['fas', 'eye']\" class=\"mr-2\" />\n        <span>查看详情</span>\n      </button>\n    </div>\n    \n    <!-- 编辑表单 - 独立模态框 -->\n    <teleport to=\"body\">\n      <div v-if=\"isEditing\" class=\"fixed inset-0 bg-black/50 flex items-center justify-center z-50 p-4\" @click.self=\"cancelEdit\">\n        <div class=\"bg-white rounded-2xl shadow-xl w-full max-w-md max-h-[90vh] overflow-y-auto\" @click.stop>\n          <div class=\"p-6 bg-gradient-to-br from-indigo-50/80 via-white/90 to-purple-50/80 backdrop-blur-sm rounded-2xl\">\n            <div class=\"flex justify-between items-center mb-4 pb-2 border-b border-indigo-100/50\">\n              <h3 class=\"text-lg font-bold text-indigo-900 flex items-center\">\n                <div class=\"w-6 h-6 rounded-full bg-gradient-to-r from-indigo-500/10 to-purple-500/10 flex items-center justify-center mr-2 shadow-sm\">\n                  <font-awesome-icon icon=\"edit\" class=\"text-indigo-600 text-xs\" />\n                </div>\n                编辑搭配信息\n              </h3>\n              <button @click=\"cancelEdit\" class=\"w-8 h-8 rounded-full bg-white/80 backdrop-blur-sm flex items-center justify-center text-gray-500 hover:text-indigo-600 hover:bg-indigo-500/10 transition-all duration-300 shadow-sm border border-indigo-100/50\">\n                <font-awesome-icon icon=\"times\" class=\"text-sm\" />\n              </button>\n            </div>\n            \n            <div class=\"space-y-5\">\n        <div>\n          <label class=\"block text-sm font-medium text-indigo-700 mb-2 flex items-center\">\n            <div class=\"w-5 h-5 rounded-full bg-gradient-to-r from-indigo-500/10 to-purple-500/10 flex items-center justify-center mr-2 shadow-sm\">\n              <font-awesome-icon icon=\"tag\" class=\"text-indigo-600 text-xs\" />\n            </div>\n            搭配名称\n          </label>\n          <div class=\"relative\">\n            <input\n              v-model=\"editOutfit.name\"\n              type=\"text\"\n              class=\"w-full px-4 py-2.5 bg-white/70 backdrop-blur-sm border bord