{"ast":null,"code":"function _typeof(o) { \"@babel/helpers - typeof\"; return _typeof = \"function\" == typeof Symbol && \"symbol\" == typeof Symbol.iterator ? function (o) { return typeof o; } : function (o) { return o && \"function\" == typeof Symbol && o.constructor === Symbol && o !== Symbol.prototype ? \"symbol\" : typeof o; }, _typeof(o); }\nfunction ownKeys(e, r) { var t = Object.keys(e); if (Object.getOwnPropertySymbols) { var o = Object.getOwnPropertySymbols(e); r && (o = o.filter(function (r) { return Object.getOwnPropertyDescriptor(e, r).enumerable; })), t.push.apply(t, o); } return t; }\nfunction _objectSpread(e) { for (var r = 1; r < arguments.length; r++) { var t = null != arguments[r] ? arguments[r] : {}; r % 2 ? ownKeys(Object(t), !0).forEach(function (r) { _defineProperty(e, r, t[r]); }) : Object.getOwnPropertyDescriptors ? Object.defineProperties(e, Object.getOwnPropertyDescriptors(t)) : ownKeys(Object(t)).forEach(function (r) { Object.defineProperty(e, r, Object.getOwnPropertyDescriptor(t, r)); }); } return e; }\nfunction _defineProperty(e, r, t) { return (r = _toPropertyKey(r)) in e ? Object.defineProperty(e, r, { value: t, enumerable: !0, configurable: !0, writable: !0 }) : e[r] = t, e; }\nfunction _toPropertyKey(t) { var i = _toPrimitive(t, \"string\"); return \"symbol\" == _typeof(i) ? i : i + \"\"; }\nfunction _toPrimitive(t, r) { if (\"object\" != _typeof(t) || !t) return t; var e = t[Symbol.toPrimitive]; if (void 0 !== e) { var i = e.call(t, r || \"default\"); if (\"object\" != _typeof(i)) return i; throw new TypeError(\"@@toPrimitive must return a primitive value.\"); } return (\"string\" === r ? String : Number)(t); }\nimport { ref, reactive, computed } from 'vue';\n\n// Props定义\n\nexport default {\n  __name: 'OutfitCard',\n  props: {\n    outfit: {\n      type: Object,\n      required: true\n    }\n  },\n  emits: ['load-outfit', 'delete-outfit', 'edit-outfit'],\n  setup: function setup(__props, _ref) {\n    var __expose = _ref.expose,\n      __emit = _ref.emit;\n    __expose();\n    var props = __props;\n\n    // 事件定义\n    var emit = __emit;\n\n    // 状态管理\n    var expanded = ref(false);\n    var hoveredIndex = ref(-1);\n    var infoExpanded = ref(false);\n\n    // 切换展开状态\n    var toggleExpanded = function toggleExpanded() {\n      expanded.value = !expanded.value;\n      if (!expanded.value) {\n        hoveredIndex.value = -1;\n      }\n    };\n\n    // 重置堆叠状态\n    var resetStack = function resetStack() {\n      if (!expanded.value) {\n        hoveredIndex.value = -1;\n      }\n    };\n\n    // 切换信息展开状态\n    var toggleInfoExpanded = function toggleInfoExpanded() {\n      infoExpanded.value = !infoExpanded.value;\n    };\n\n    // 格式化时间显示\n    var formatTime = function formatTime(timestamp) {\n      if (!timestamp) return '';\n      var date = new Date(timestamp);\n      var now = new Date();\n      var diffMs = now - date;\n      var diffMins = Math.floor(diffMs / 60000);\n      var diffHours = Math.floor(diffMs / 3600000);\n      var diffDays = Math.floor(diffMs / 86400000);\n      if (diffMins < 1) return '刚刚';\n      if (diffMins < 60) return \"\".concat(diffMins, \"\\u5206\\u949F\\u524D\");\n      if (diffHours < 24) return \"\".concat(diffHours, \"\\u5C0F\\u65F6\\u524D\");\n      if (diffDays < 7) return \"\".concat(diffDays, \"\\u5929\\u524D\");\n\n      // 超过一周显示具体日期\n      var year = date.getFullYear();\n      var month = date.getMonth() + 1;\n      var day = date.getDate();\n      if (year === now.getFullYear()) {\n        return \"\".concat(month, \"\\u6708\").concat(day, \"\\u65E5\");\n      }\n      return \"\".concat(year, \"\\u5E74\").concat(month, \"\\u6708\").concat(day, \"\\u65E5\");\n    };\n\n    // 获取标签样式\n    var getTagStyle = function getTagStyle(tag) {\n      // 根据标签内容返回不同的样式\n      var tagLower = tag.toLowerCase();\n\n      // 季节标签\n      if (['春', '夏', '秋', '冬', '春季', '夏季', '秋季', '冬季'].includes(tagLower)) {\n        return 'bg-green-100 text-green-800';\n      }\n\n      // 场景标签\n      if (['日常', '通勤', '约会', '聚会', '商务', '休闲', '运动', '旅行'].includes(tagLower)) {\n        return 'bg-blue-100 text-blue-800';\n      }\n\n      // 风格标签\n      if (['简约', '复古', '潮流', '优雅', '可爱', '酷帅', '文艺', '甜美'].includes(tagLower)) {\n        return 'bg-purple-100 text-purple-800';\n      }\n\n      // 默认样式\n      return 'bg-indigo-100 text-indigo-800';\n    };\n\n    // 编辑状态\n    var isEditing = ref(false);\n    var editOutfit = reactive({});\n\n    // 切换编辑模式\n    function toggleEditMode() {\n      if (!isEditing.value) {\n        // 进入编辑模式，复制当前数据\n        Object.assign(editOutfit, {\n          id: props.outfit.id,\n          name: props.outfit.name,\n          scene: props.outfit.scene || ''\n        });\n      }\n      isEditing.value = !isEditing.value;\n    }\n\n    // 保存编辑\n    function saveEdit() {\n      // 验证数据\n      if (!editOutfit.name.trim()) {\n        alert('搭配名称不能为空');\n        return;\n      }\n\n      // 发送编辑事件\n      emit('edit-outfit', _objectSpread({}, editOutfit));\n      // 退出编辑模式\n      isEditing.value = false;\n    }\n\n    // 取消编辑\n    function cancelEdit() {\n      isEditing.value = false;\n    }\n\n    // 获取搭配评分（模拟）\n    function getOutfitRating(outfit) {\n      // 基于衣物数量和搭配多样性计算评分\n      if (!outfit.items || outfit.items.length === 0) return 0;\n\n      // 基础分 5 分\n      var baseScore = 5;\n\n      // 衣物数量加分 (最多 3 分)\n      var quantityBonus = Math.min(Math.floor(outfit.items.length / 2), 3);\n\n      // 搭配多样性加分 (最多 2 分)\n      var types = new Set(outfit.items.map(function (item) {\n        return item.type.split(' · ')[0];\n      }));\n      var diversityBonus = Math.min(types.size - 1, 2);\n      return baseScore + quantityBonus + diversityBonus;\n    }\n    var __returned__ = {\n      props: props,\n      emit: emit,\n      expanded: expanded,\n      hoveredIndex: hoveredIndex,\n      infoExpanded: infoExpanded,\n      toggleExpanded: toggleExpanded,\n      resetStack: resetStack,\n      toggleInfoExpanded: toggleInfoExpanded,\n      formatTime: formatTime,\n      getTagStyle: getTagStyle,\n      isEditing: isEditing,\n      editOutfit: editOutfit,\n      toggleEditMode: toggleEditMode,\n      saveEdit: saveEdit,\n      cancelEdit: cancelEdit,\n      getOutfitRating: getOutfitRating,\n      ref: ref,\n      reactive: reactive,\n      computed: computed\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","computed","props","__props","emit","__emit","expanded","hoveredIndex","infoExpanded","toggleExpanded","value","resetStack","toggleInfoExpanded","formatTime","timestamp","date","Date","now","diffMs","diffMins","Math","floor","diffHours","diffDays","concat","year","getFullYear","month","getMonth","day","getDate","getTagStyle","tag","tagLower","toLowerCase","includes","isEditing","editOutfit","toggleEditMode","Object","assign","id","outfit","name","scene","saveEdit","trim","alert","_objectSpread","cancelEdit","getOutfitRating","items","length","baseScore","quantityBonus","min","types","Set","map","item","type","split","diversityBonus","size"],"sources":["/Users/sunxiaokai/Desktop/stylevault-vue-project/src/components/features/inspiration/OutfitCard.vue"],"sourcesContent":["<template>\n  <div class=\"bg-gradient-to-br from-indigo-50 via-white to-purple-50 rounded-2xl overflow-hidden shadow-lg hover:shadow-2xl transition-all duration-500 group border border-indigo-100/80 flex flex-col transform hover:-translate-y-1.5 h-full\">\n    <!-- 搭配预览 - 优化后的卡片顶部 -->\n    <div class=\"relative overflow-hidden\">\n      <!-- 卡片顶部渐变背景 -->\n      <div class=\"absolute inset-0 bg-gradient-to-br from-indigo-500/15 via-purple-500/10 to-white/80 transform transition-transform duration-700 group-hover:scale-110\"></div>\n\n      <!-- 装饰元素 - 增强卡片深度感 -->\n      <div class=\"absolute -top-12 -right-12 w-28 h-28 bg-gradient-to-r from-indigo-400/20 to-purple-400/20 rounded-full blur-2xl opacity-70 animate-pulse-slow\"></div>\n      <div class=\"absolute -bottom-12 -left-12 w-28 h-28 bg-gradient-to-r from-purple-400/20 to-indigo-400/20 rounded-full blur-2xl opacity-70 animate-pulse-slow animation-delay-1000\"></div>\n      <div class=\"absolute top-1/4 left-1/4 w-4 h-4 rounded-full bg-indigo-400/30 animate-ping\"></div>\n      <div class=\"absolute bottom-1/3 right-1/3 w-3 h-3 rounded-full bg-purple-400/30 animate-ping animation-delay-700\"></div>\n\n      <!-- 卡片内容 -->\n      <div class=\"p-5 h-56 flex flex-col relative z-10\">\n        <!-- 顶部信息区 -->\n        <div class=\"flex justify-between items-start mb-3\">\n          <div class=\"flex-1 min-w-0\">\n            <h4 class=\"font-bold text-indigo-900 text-lg truncate pr-2 group-hover:text-indigo-600 transition-colors flex items-center\">\n              {{ outfit.name }}\n              <span v-if=\"outfit?.isFavorite\" class=\"ml-2 text-amber-500\" title=\"收藏搭配\">\n                <font-awesome-icon icon=\"star\" class=\"text-xs\" />\n              </span>\n            </h4>\n            \n            <!-- 场景标签 -->\n            <div v-if=\"outfit?.scene\" class=\"flex items-center mt-1.5\">\n              <div class=\"w-5 h-5 rounded-full bg-gradient-to-r from-indigo-500/10 to-purple-500/10 flex items-center justify-center mr-1.5 shadow-sm\">\n                <font-awesome-icon icon=\"map-marker-alt\" class=\"text-indigo-600 text-xs\" />\n              </div>\n              <span class=\"text-xs text-indigo-600 font-medium bg-white/70 backdrop-blur-sm px-2 py-1 rounded-full border border-indigo-100/50\">\n                {{ outfit?.scene || '' }}\n              </span>\n            </div>\n            \n            <!-- 时间信息 - 默认隐藏，悬停时显示 -->\n            <div class=\"flex items-center mt-1.5 opacity-0 group-hover:opacity-100 transition-opacity duration-300\">\n              <div class=\"w-5 h-5 rounded-full bg-gradient-to-r from-indigo-500/10 to-purple-500/10 flex items-center justify-center mr-1.5 shadow-sm\">\n                <font-awesome-icon icon=\"clock\" class=\"text-indigo-600 text-xs\" />\n              </div>\n              <span class=\"text-xs text-indigo-600 font-medium\">\n                {{ formatTime(outfit?.createdAt) }}{{ outfit?.updatedAt && outfit?.updatedAt !== outfit?.createdAt ? ' · 更新于' + formatTime(outfit?.updatedAt) : '' }}\n              </span>\n            </div>\n          </div>\n          \n          <!-- 分层信息按钮 -->\n          <button @click=\"toggleInfoExpanded\" \n                  class=\"w-8 h-8 rounded-full bg-white/70 backdrop-blur-sm flex items-center justify-center text-indigo-500 hover:bg-indigo-500 hover:text-white transition-all duration-300 shadow-sm hover:shadow-md border border-indigo-100/50 flex-shrink-0 ml-2\"\n                  title=\"更多信息\">\n            <font-awesome-icon :icon=\"['fas', infoExpanded ? 'chevron-up' : 'chevron-down']\" class=\"text-xs transition-transform duration-300\" />\n          </button>\n        </div>\n        \n        <!-- 扩展信息区域 -->\n        <div v-if=\"infoExpanded\" class=\"mb-3 overflow-hidden transition-all duration-300\">\n          <!-- 标签区域 -->\n          <div class=\"flex flex-wrap gap-1.5 mb-2\" v-if=\"outfit?.tags && outfit.tags.length > 0\">\n            <span v-for=\"(tag, index) in outfit.tags.slice(0, 5)\" :key=\"index\"\n                  class=\"text-xs px-2 py-1 rounded-full transition-all duration-300 hover:scale-105\"\n                  :class=\"getTagStyle(tag)\">\n              {{ tag }}\n            </span>\n            <span v-if=\"outfit.tags.length > 5\" class=\"text-xs px-2 py-1 rounded-full bg-indigo-100 text-indigo-700\">\n              +{{ outfit.tags.length - 5 }}\n            </span>\n          </div>\n          \n          <!-- 评分信息 -->\n          <div class=\"flex items-center\" v-if=\"outfit?.rating !== undefined\">\n            <div class=\"flex items-center mr-2\">\n              <font-awesome-icon v-for=\"i in 5\" :key=\"i\" \n                               :icon=\"['fas', 'star']\" \n                               class=\"text-xs\" \n                               :class=\"i <= outfit.rating ? 'text-amber-400' : 'text-gray-300'\" />\n            </div>\n            <span class=\"text-xs text-indigo-600 font-medium\">{{ outfit.rating }}/5</span>\n          </div>\n        </div>\n\n        <!-- 衣物预览区 - 堆叠效果 -->\n        <div class=\"flex-1 flex items-center justify-center relative\" @mouseleave=\"resetStack\" style=\"height: 220px;\">\n          <div v-if=\"(outfit?.items?.length || 0) === 0\" class=\"text-center text-indigo-400\">\n            <div class=\"w-16 h-16 rounded-2xl bg-gradient-to-br from-indigo-100/80 to-purple-100/50 backdrop-blur-sm flex items-center justify-center mx-auto mb-3 shadow-lg border border-indigo-100/50\">\n              <font-awesome-icon icon=\"tshirt\" class=\"text-indigo-500 text-2xl\" />\n            </div>\n            <p class=\"text-sm text-indigo-500\">暂无衣物</p>\n          </div>\n          <div v-else class=\"relative w-full h-full overflow-visible\">\n            <!-- 衣物图片堆叠效果 - 重新设计 -->\n            <div class=\"relative w-full h-full flex items-center justify-center\">\n              <template v-for=\"(item, idx) in (outfit?.items || []).slice(0, 4)\" :key=\"idx\">\n              <div \n                v-if=\"item.img\"\n                  class=\"absolute w-28 h-36 bg-gradient-to-br from-white to-indigo-50/50 rounded-xl shadow-lg overflow-hidden transition-all duration-500 ease-out cursor-pointer border-2 border-white/80 backdrop-blur-sm\"\n                  :class=\"{ 'ring-2 ring-indigo-400/30 ring-offset-1': hoveredIndex === idx }\"\n                  :style=\"{\n                    zIndex: 30 - idx,\n                    transform: expanded ? \n                      (hoveredIndex === idx ? 'translateY(0) translateX(0) rotate(0) scale(1.1)' : \n                       `translateY(${idx * 2}px) translateX(${idx % 2 === 0 ? -idx * 6 : idx * 6}px) rotate(${idx % 2 === 0 ? -1 - idx * 0.5 : 1 + idx * 0.5}deg)`) :\n                      `translateY(${idx * 3}px) translateX(${idx % 2 === 0 ? -idx * 8 : idx * 8}px) rotate(${idx % 2 === 0 ? -2 - idx : 2 + idx}deg)`,\n                    opacity: expanded ? (hoveredIndex === idx ? 1 : 0.8) : 1,\n                    filter: expanded && hoveredIndex !== idx ? 'blur(1px)' : 'none'\n                  }\"\n                @mouseenter=\"hoveredIndex = idx\"\n                @click=\"toggleExpanded\">\n                  <!-- 图片容器 -->\n                  <div class=\"relative w-full h-full overflow-hidden\">\n                    <img \n                      :src=\"item.img\" \n                      :alt=\"item.name\" \n                      class=\"w-full h-full object-cover transition-transform duration-500\"\n                      :class=\"{ 'scale-110': hoveredIndex === idx }\"\n                    >\n                    <!-- 悬停时的渐变遮罩 -->\n                    <div class=\"absolute inset-0 bg-gradient-to-t from-black/40 via-transparent to-transparent opacity-0 transition-opacity duration-400\" \n                         :class=\"{ 'opacity-100': hoveredIndex === idx }\"></div>\n                    \n                    <!-- 物品标签 -->\n                    <div class=\"absolute bottom-0 left-0 right-0 bg-gradient-to-t from-black/70 to-transparent p-2 text-white transition-opacity duration-300\"\n                         :class=\"{ 'opacity-0': hoveredIndex !== idx && !expanded, 'opacity-100': hoveredIndex === idx || expanded }\">\n                      <div class=\"text-xs font-medium truncate\">{{ item.name || `物品 ${idx + 1}` }}</div>\n                    </div>\n                  </div>\n                </div>\n              </template>\n            </div>\n            \n            <!-- 更多衣物指示器 -->\n            <div \n              v-if=\"(outfit?.items?.length || 0) > 4\" \n              class=\"absolute bottom-1 right-1 bg-gradient-to-r from-indigo-500 to-purple-500 text-white rounded-full w-10 h-10 flex items-center justify-center shadow-md text-sm font-bold border-2 border-white z-40 transition-all duration-300 hover:scale-110 hover:shadow-lg cursor-pointer flex-shrink-0\"\n              :class=\"{ 'animate-pulse': !expanded }\"\n              @click=\"toggleExpanded\"\n            >\n              +{{ (outfit?.items?.length || 0) - 4 }}\n            </div>\n            \n            <!-- 提示 -->\n            <div \n              v-if=\"!expanded\" \n              class=\"absolute bottom-0 left-0 right-0 text-center text-xs font-medium text-indigo-600/70 transition-opacity duration-300\"\n            >\n              点击展开查看更多\n            </div>\n\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <!-- 优化的搭配信息 -->\n    <div class=\"p-5 flex-1 flex flex-col bg-gradient-to-br from-indigo-50/80 via-white/90 to-purple-50/80 backdrop-blur-sm\" style=\"min-height: 200px;\">\n      <!-- 衣物数量统计 -->\n      <div class=\"mb-4 flex items-center justify-between\">\n        <span class=\"text-sm font-medium text-indigo-700 flex items-center bg-white/70 backdrop-blur-sm px-3 py-1.5 rounded-full shadow-sm border border-indigo-100/50\">\n          <div class=\"w-5 h-5 rounded-full bg-gradient-to-r from-indigo-500/10 to-purple-500/10 flex items-center justify-center mr-2 shadow-sm\">\n            <font-awesome-icon icon=\"layer-group\" class=\"text-indigo-600 text-xs\" />\n          </div>\n          {{ outfit?.items?.length || 0 }}件衣物\n        </span>\n        <!-- 操作按钮 -->\n        <div class=\"flex gap-2\">\n          <button @click=\"toggleEditMode\"\n                  class=\"w-9 h-9 rounded-full bg-white/80 backdrop-blur-sm flex items-center justify-center text-indigo-500 hover:bg-indigo-500 hover:text-white transition-all duration-300 shadow-md hover:shadow-lg border border-indigo-100/50\"\n                  title=\"编辑搭配\">\n            <font-awesome-icon icon=\"edit\" class=\"text-xs\" />\n          </button>\n          <button @click=\"$emit('load-outfit', outfit)\"\n                  class=\"w-9 h-9 rounded-full bg-white/80 backdrop-blur-sm flex items-center justify-center text-indigo-600 hover:bg-indigo-600 hover:text-white transition-all duration-300 shadow-md hover:shadow-lg border border-indigo-100/50\"\n                  title=\"加载搭配\">\n            <font-awesome-icon icon=\"redo\" class=\"text-xs\" />\n          </button>\n          <button @click=\"$emit('delete-outfit', outfit.id)\"\n                  class=\"w-9 h-9 rounded-full bg-white/80 backdrop-blur-sm flex items-center justify-center text-red-500 hover:bg-red-500 hover:text-white transition-all duration-300 shadow-md hover:shadow-lg border border-indigo-100/50\"\n                  title=\"删除搭配\">\n            <font-awesome-icon icon=\"trash\" class=\"text-xs\" />\n          </button>\n        </div>\n      </div>\n\n      <!-- 编辑表单 -->\n      <div v-if=\"isEditing\" class=\"space-y-4 mt-auto\">\n        <div>\n          <label class=\"block text-sm font-medium text-indigo-700 mb-2 flex items-center\">\n            <div class=\"w-5 h-5 rounded-full bg-gradient-to-r from-indigo-500/10 to-purple-500/10 flex items-center justify-center mr-2 shadow-sm\">\n              <font-awesome-icon icon=\"tag\" class=\"text-indigo-600 text-xs\" />\n            </div>\n            搭配名称\n          </label>\n          <input \n            v-model=\"editOutfit.name\" \n            type=\"text\" \n            class=\"w-full px-3 py-2 border border-neutral-300 rounded-lg focus:ring-2 focus:ring-primary focus:border-primary text-sm\"\n            placeholder=\"输入搭配名称\"\n          />\n        </div>\n        <div>\n          <label class=\"block text-xs font-medium text-neutral-700 mb-1\">适用场景</label>\n          <input \n            v-model=\"editOutfit.scene\" \n            type=\"text\" \n            class=\"w-full px-3 py-2 border border-neutral-300 rounded-lg focus:ring-2 focus:ring-primary focus:border-primary text-sm\"\n            placeholder=\"输入适用场景\"\n          />\n        </div>\n        <div class=\"flex gap-2\">\n          <button @click=\"saveEdit\"\n                  class=\"flex-1 bg-primary hover:bg-primary/90 text-white font-medium py-2 rounded-lg transition-colors flex items-center justify-center\">\n            <font-awesome-icon :icon=\"['fas', 'save']\" class=\"mr-1 text-xs\" />\n            保存\n          </button>\n          <button @click=\"cancelEdit\"\n                  class=\"flex-1 bg-neutral-100 hover:bg-neutral-200 text-neutral-700 font-medium py-2 rounded-lg transition-colors flex items-center justify-center\">\n            取消\n          </button>\n        </div>\n      </div>\n\n      <!-- 主要操作按钮 -->\n      <button v-else @click=\"$emit('load-outfit', outfit)\"\n              class=\"w-full bg-gradient-to-r from-primary to-secondary hover:from-primary/90 hover:to-secondary/90 text-white font-medium py-2.5 rounded-xl transition-all duration-300 flex items-center justify-center shadow-md hover:shadow-lg transform hover:-translate-y-1 active:translate-y-0 mt-auto\">\n        <font-awesome-icon :icon=\"['fas', 'eye']\" class=\"mr-2\" />\n        <span>查看详情</span>\n      </button>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, reactive, computed } from 'vue'\n\n// Props定义\nconst props = defineProps({\n  outfit: {\n    type: Object,\n    required: true\n  }\n})\n\n// 事件定义\nconst emit = defineEmits(['load-outfit', 'delete-outfit', 'edit-outfit'])\n\n// 状态管理\nconst expanded = ref(false);\nconst hoveredIndex = ref(-1);\nconst infoExpanded = ref(false);\n\n// 切换展开状态\nconst toggleExpanded = () => {\n  expanded.value = !expanded.value;\n  if (!expanded.value) {\n    hoveredIndex.value = -1;\n  }\n};\n\n// 重置堆叠状态\nconst resetStack = () => {\n  if (!expanded.value) {\n    hoveredIndex.value = -1;\n  }\n};\n\n// 切换信息展开状态\nconst toggleInfoExpanded = () => {\n  infoExpanded.value = !infoExpanded.value;\n};\n\n// 格式化时间显示\nconst formatTime = (timestamp) => {\n  if (!timestamp) return '';\n  \n  const date = new Date(timestamp);\n  const now = new Date();\n  const diffMs = now - date;\n  const diffMins = Math.floor(diffMs / 60000);\n  const diffHours = Math.floor(diffMs / 3600000);\n  const diffDays = Math.floor(diffMs / 86400000);\n  \n  if (diffMins < 1) return '刚刚';\n  if (diffMins < 60) return `${diffMins}分钟前`;\n  if (diffHours < 24) return `${diffHours}小时前`;\n  if (diffDays < 7) return `${diffDays}天前`;\n  \n  // 超过一周显示具体日期\n  const year = date.getFullYear();\n  const month = date.getMonth() + 1;\n  const day = date.getDate();\n  \n  if (year === now.getFullYear()) {\n    return `${month}月${day}日`;\n  }\n  \n  return `${year}年${month}月${day}日`;\n};\n\n// 获取标签样式\nconst getTagStyle = (tag) => {\n  // 根据标签内容返回不同的样式\n  const tagLower = tag.toLowerCase();\n  \n  // 季节标签\n  if (['春', '夏', '秋', '冬', '春季', '夏季', '秋季', '冬季'].includes(tagLower)) {\n    return 'bg-green-100 text-green-800';\n  }\n  \n  // 场景标签\n  if (['日常', '通勤', '约会', '聚会', '商务', '休闲', '运动', '旅行'].includes(tagLower)) {\n    return 'bg-blue-100 text-blue-800';\n  }\n  \n  // 风格标签\n  if (['简约', '复古', '潮流', '优雅', '可爱', '酷帅', '文艺', '甜美'].includes(tagLower)) {\n    return 'bg-purple-100 text-purple-800';\n  }\n  \n  // 默认样式\n  return 'bg-indigo-100 text-indigo-800';\n};\n\n// 编辑状态\nconst isEditing = ref(false)\nconst editOutfit = reactive({})\n\n// 切换编辑模式\nfunction toggleEditMode() {\n  if (!isEditing.value) {\n    // 进入编辑模式，复制当前数据\n    Object.assign(editOutfit, {\n      id: props.outfit.id,\n      name: props.outfit.name,\n      scene: props.outfit.scene || ''\n    })\n  }\n  isEditing.value = !isEditing.value\n}\n\n// 保存编辑\nfunction saveEdit() {\n  // 验证数据\n  if (!editOutfit.name.trim()) {\n    alert('搭配名称不能为空')\n    return\n  }\n\n  // 发送编辑事件\n  emit('edit-outfit', { ...editOutfit })\n  // 退出编辑模式\n  isEditing.value = false\n}\n\n// 取消编辑\nfunction cancelEdit() {\n  isEditing.value = false\n}\n\n// 获取搭配评分（模拟）\nfunction getOutfitRating(outfit) {\n  // 基于衣物数量和搭配多样性计算评分\n  if (!outfit.items || outfit.items.length === 0) return 0\n\n  // 基础分 5 分\n  let baseScore = 5\n\n  // 衣物数量加分 (最多 3 分)\n  const quantityBonus = Math.min(Math.floor(outfit.items.length / 2), 3)\n\n  // 搭配多样性加分 (最多 2 分)\n  const types = new Set(outfit.items.map(item => item.type.split(' · ')[0]))\n  const diversityBonus = Math.min(types.size - 1, 2)\n\n  return baseScore + quantityBonus + diversityBonus\n}\n</script>\n\n<style scoped>\n/* 自定义滚动条样式 */\n.scrollbar-thin::-webkit-scrollbar {\n  width: 4px;\n}\n\n.scrollbar-thin::-webkit-scrollbar-track {\n  background: transparent;\n}\n\n.scrollbar-thin::-webkit-scrollbar-thumb {\n  background: rgba(0, 0, 0, 0.1);\n  border-radius: 2px;\n}\n\n.scrollbar-thin::-webkit-scrollbar-thumb:hover {\n  background: rgba(0, 0, 0, 0.2);\n}\n\n/* 悬停动画增强 */\n.group:hover .text-primary {\n  color: theme('colors.primary', #3b82f6);\n}\n\n/* 卡片悬停效果增强 */\n.shadow-md {\n  box-shadow: 0 4px 6px -1px rgba(0, 0, 0, 0.05), 0 2px 4px -1px rgba(0, 0, 0, 0.03);\n}\n\n.shadow-lg {\n  box-shadow: 0 10px 15px -3px rgba(0, 0, 0, 0.08), 0 4px 6px -2px rgba(0, 0, 0, 0.04);\n}\n</style>\n"],"mappings":";;;;;;AAuOA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,QAAQ,QAAQ,KAAI;;AAE5C;;;;;;;;;;;;;;;IACA,IAAMC,KAAK,GAAGC,OAAA;;IAOd;IACA,IAAMC,IAAI,GAAGC,MAAA;;IAEb;IACA,IAAMC,QAAQ,GAAGP,GAAG,CAAC,KAAK,CAAC;IAC3B,IAAMQ,YAAY,GAAGR,GAAG,CAAC,CAAC,CAAC,CAAC;IAC5B,IAAMS,YAAY,GAAGT,GAAG,CAAC,KAAK,CAAC;;IAE/B;IACA,IAAMU,cAAc,GAAG,SAAjBA,cAAcA,CAAA,EAAS;MAC3BH,QAAQ,CAACI,KAAK,GAAG,CAACJ,QAAQ,CAACI,KAAK;MAChC,IAAI,CAACJ,QAAQ,CAACI,KAAK,EAAE;QACnBH,YAAY,CAACG,KAAK,GAAG,CAAC,CAAC;MACzB;IACF,CAAC;;IAED;IACA,IAAMC,UAAU,GAAG,SAAbA,UAAUA,CAAA,EAAS;MACvB,IAAI,CAACL,QAAQ,CAACI,KAAK,EAAE;QACnBH,YAAY,CAACG,KAAK,GAAG,CAAC,CAAC;MACzB;IACF,CAAC;;IAED;IACA,IAAME,kBAAkB,GAAG,SAArBA,kBAAkBA,CAAA,EAAS;MAC/BJ,YAAY,CAACE,KAAK,GAAG,CAACF,YAAY,CAACE,KAAK;IAC1C,CAAC;;IAED;IACA,IAAMG,UAAU,GAAG,SAAbA,UAAUA,CAAIC,SAAS,EAAK;MAChC,IAAI,CAACA,SAAS,EAAE,OAAO,EAAE;MAEzB,IAAMC,IAAI,GAAG,IAAIC,IAAI,CAACF,SAAS,CAAC;MAC