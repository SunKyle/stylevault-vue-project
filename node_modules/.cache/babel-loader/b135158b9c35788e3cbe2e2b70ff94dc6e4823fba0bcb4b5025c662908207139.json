{"ast":null,"code":"import { ref, reactive, computed } from 'vue';\n\n// Props定义\n\nexport default {\n  __name: 'OutfitCard',\n  props: {\n    outfit: {\n      type: Object,\n      required: true\n    },\n    isEditing: {\n      type: Boolean,\n      \"default\": false\n    }\n  },\n  emits: ['load-outfit', 'delete-outfit', 'edit-start'],\n  setup: function setup(__props, _ref) {\n    var __expose = _ref.expose,\n      __emit = _ref.emit;\n    __expose();\n    var props = __props;\n\n    // 事件定义\n    var emit = __emit;\n\n    // 季节选项映射\n    var seasonOptions = [{\n      value: 'spring',\n      label: '春季'\n    }, {\n      value: 'summer',\n      label: '夏季'\n    }, {\n      value: 'autumn',\n      label: '秋季'\n    }, {\n      value: 'winter',\n      label: '冬季'\n    }];\n\n    // 风格选项映射\n    var styleOptions = [{\n      value: 'casual',\n      label: '休闲'\n    }, {\n      value: 'formal',\n      label: '正式'\n    }, {\n      value: 'business',\n      label: '商务'\n    }, {\n      value: 'street',\n      label: '街头'\n    }, {\n      value: 'vintage',\n      label: '复古'\n    }, {\n      value: 'minimalist',\n      label: '极简'\n    }];\n\n    // 获取季节标签\n    function getSeasonLabel(value) {\n      var season = seasonOptions.find(function (option) {\n        return option.value === value;\n      });\n      return season ? season.label : value;\n    }\n\n    // 获取风格标签\n    function getStyleLabel(value) {\n      var style = styleOptions.find(function (option) {\n        return option.value === value;\n      });\n      return style ? style.label : value;\n    }\n\n    // 获取衣物图标\n    function getClothingIcon(category) {\n      var iconMap = {\n        'top': 'tshirt',\n        'bottom': 'socks',\n        'shoe': 'shoe-prints',\n        'accessory': 'glasses',\n        'outerwear': 'vest',\n        'dress': 'feminine',\n        'underwear': 'mitten',\n        'default': 'tag'\n      };\n      return iconMap[category] || iconMap['default'];\n    }\n\n    // 切换编辑模式\n    function toggleEditMode() {\n      emit('edit-start');\n    }\n    var __returned__ = {\n      props: props,\n      emit: emit,\n      seasonOptions: seasonOptions,\n      styleOptions: styleOptions,\n      getSeasonLabel: getSeasonLabel,\n      getStyleLabel: getStyleLabel,\n      getClothingIcon: getClothingIcon,\n      toggleEditMode: toggleEditMode,\n      ref: ref,\n      reactive: reactive,\n      computed: computed\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","reactive","computed","props","__props","emit","__emit","seasonOptions","value","label","styleOptions","getSeasonLabel","season","find","option","getStyleLabel","style","getClothingIcon","category","iconMap","toggleEditMode"],"sources":["/Users/sunxiaokai/Desktop/stylevault-vue-project/src/components/features/inspiration/OutfitCard.vue"],"sourcesContent":["<template>\n  <div class=\"bg-gradient-to-br from-indigo-50 via-white to-purple-50 rounded-2xl overflow-hidden shadow-lg hover:shadow-2xl transition-all duration-500 group border border-indigo-100/80 flex flex-col transform hover:-translate-y-1.5 h-full\">\n    <!-- 搭配预览 - 优化后的卡片顶部 -->\n    <div class=\"relative overflow-hidden\">\n      <!-- 卡片顶部渐变背景 -->\n      <div class=\"absolute inset-0 bg-gradient-to-br from-indigo-500/15 via-purple-500/10 to-white/80 transform transition-transform duration-700 group-hover:scale-110\"></div>\n\n      <!-- 装饰元素 - 增强卡片深度感 -->\n      <div class=\"absolute -top-12 -right-12 w-28 h-28 bg-gradient-to-r from-indigo-400/20 to-purple-400/20 rounded-full blur-2xl opacity-70 animate-pulse-slow\"></div>\n      <div class=\"absolute -bottom-12 -left-12 w-28 h-28 bg-gradient-to-r from-purple-400/20 to-indigo-400/20 rounded-full blur-2xl opacity-70 animate-pulse-slow animation-delay-1000\"></div>\n      <div class=\"absolute top-1/4 left-1/4 w-4 h-4 rounded-full bg-indigo-400/30 animate-ping\"></div>\n      <div class=\"absolute bottom-1/3 right-1/3 w-3 h-3 rounded-full bg-purple-400/30 animate-ping animation-delay-700\"></div>\n\n      <!-- 卡片内容 -->\n      <div class=\"p-5 h-56 flex flex-col relative z-10\">\n        <!-- 顶部信息区 -->\n        <div class=\"flex justify-between items-start mb-3\">\n          <div>\n            <h4 class=\"font-bold text-indigo-900 text-lg truncate pr-2 group-hover:text-indigo-600 transition-colors\">\n              {{ outfit.name }}\n            </h4>\n            <div class=\"flex items-center gap-2 mt-1\">\n              <span v-if=\"outfit.scene\" class=\"bg-indigo-100 text-indigo-700 px-2 py-0.5 rounded-full text-xs font-medium\">\n                {{ outfit.scene }}\n              </span>\n              <span v-if=\"outfit.season\" class=\"bg-green-100 text-green-700 px-2 py-0.5 rounded-full text-xs font-medium\">\n                {{ getSeasonLabel(outfit.season) }}\n              </span>\n              <span v-if=\"outfit.style\" class=\"bg-purple-100 text-purple-700 px-2 py-0.5 rounded-full text-xs font-medium\">\n                {{ getStyleLabel(outfit.style) }}\n              </span>\n            </div>\n          </div>\n          <!-- 操作按钮 -->\n          <div class=\"flex gap-2\">\n            <button @click=\"toggleEditMode\"\n                    class=\"w-9 h-9 rounded-full bg-white/80 backdrop-blur-sm flex items-center justify-center text-indigo-500 hover:bg-indigo-500 hover:text-white transition-all duration-300 shadow-md hover:shadow-lg border border-indigo-100/50\"\n                    title=\"编辑搭配\">\n              <font-awesome-icon icon=\"edit\" class=\"text-xs\" />\n            </button>\n            <button @click=\"$emit('load-outfit', outfit)\"\n                    class=\"w-9 h-9 rounded-full bg-white/80 backdrop-blur-sm flex items-center justify-center text-indigo-600 hover:bg-indigo-600 hover:text-white transition-all duration-300 shadow-md hover:shadow-lg border border-indigo-100/50\"\n                    title=\"加载搭配\">\n              <font-awesome-icon icon=\"redo\" class=\"text-xs\" />\n            </button>\n            <button @click=\"$emit('delete-outfit', outfit.id)\"\n                    class=\"w-9 h-9 rounded-full bg-white/80 backdrop-blur-sm flex items-center justify-center text-red-500 hover:bg-red-500 hover:text-white transition-all duration-300 shadow-md hover:shadow-lg border border-indigo-100/50\"\n                    title=\"删除搭配\">\n              <font-awesome-icon icon=\"trash\" class=\"text-xs\" />\n            </button>\n          </div>\n        </div>\n\n        <!-- 搭配图片预览 -->\n        <div class=\"flex-1 flex items-center justify-center\">\n          <div class=\"relative w-full h-32 flex items-center justify-center\">\n            <!-- 装饰背景 -->\n            <div class=\"absolute inset-0 bg-gradient-to-br from-indigo-100/30 to-purple-100/30 rounded-xl\"></div>\n\n            <!-- 衣物图标网格 -->\n            <div v-if=\"outfit.items && outfit.items.length > 0\" class=\"relative grid grid-cols-3 gap-3 w-4/5\">\n              <div v-for=\"(item, index) in outfit.items.slice(0, 6)\" :key=\"index\"\n                   class=\"aspect-square rounded-lg bg-white/80 backdrop-blur-sm flex items-center justify-center shadow-sm border border-indigo-100/50\">\n                <font-awesome-icon :icon=\"getClothingIcon(item.category)\" class=\"text-indigo-500\" />\n              </div>\n            </div>\n\n            <!-- 空状态 -->\n            <div v-else class=\"text-center\">\n              <font-awesome-icon :icon=\"['fas', 'tshirt']\" class=\"text-indigo-300 text-2xl mb-2\" />\n              <p class=\"text-indigo-300 text-sm\">暂无衣物</p>\n            </div>\n          </div>\n        </div>\n\n        <!-- 底部信息 -->\n        <div class=\"flex justify-between items-center mt-3\">\n          <span class=\"text-xs text-indigo-500 font-medium\">\n            {{ outfit?.items?.length || 0 }}件衣物\n          </span>\n        </div>\n\n        <!-- 主要操作按钮 -->\n        <button v-if=\"!isEditing\" @click=\"$emit('load-outfit', outfit)\"\n                class=\"w-full bg-gradient-to-r from-primary to-secondary hover:from-primary/90 hover:to-secondary/90 text-white font-medium py-2.5 rounded-xl transition-all duration-300 flex items-center justify-center shadow-md hover:shadow-lg transform hover:-translate-y-1 active:translate-y-0 mt-auto\">\n          <font-awesome-icon :icon=\"['fas', 'eye']\" class=\"mr-2\" />\n          <span>查看详情</span>\n        </button>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { ref, reactive, computed } from 'vue'\n\n// Props定义\nconst props = defineProps({\n  outfit: {\n    type: Object,\n    required: true\n  },\n  isEditing: {\n    type: Boolean,\n    default: false\n  }\n})\n\n// 事件定义\nconst emit = defineEmits(['load-outfit', 'delete-outfit', 'edit-start'])\n\n// 季节选项映射\nconst seasonOptions = [\n  { value: 'spring', label: '春季' },\n  { value: 'summer', label: '夏季' },\n  { value: 'autumn', label: '秋季' },\n  { value: 'winter', label: '冬季' }\n]\n\n// 风格选项映射\nconst styleOptions = [\n  { value: 'casual', label: '休闲' },\n  { value: 'formal', label: '正式' },\n  { value: 'business', label: '商务' },\n  { value: 'street', label: '街头' },\n  { value: 'vintage', label: '复古' },\n  { value: 'minimalist', label: '极简' }\n]\n\n// 获取季节标签\nfunction getSeasonLabel(value) {\n  const season = seasonOptions.find(option => option.value === value)\n  return season ? season.label : value\n}\n\n// 获取风格标签\nfunction getStyleLabel(value) {\n  const style = styleOptions.find(option => option.value === value)\n  return style ? style.label : value\n}\n\n// 获取衣物图标\nfunction getClothingIcon(category) {\n  const iconMap = {\n    'top': 'tshirt',\n    'bottom': 'socks',\n    'shoe': 'shoe-prints',\n    'accessory': 'glasses',\n    'outerwear': 'vest',\n    'dress': 'feminine',\n    'underwear': 'mitten',\n    'default': 'tag'\n  }\n  return iconMap[category] || iconMap['default']\n}\n\n// 切换编辑模式\nfunction toggleEditMode() {\n  emit('edit-start')\n}\n</script>\n\n<style scoped>\n/* 自定义动画 */\n@keyframes pulse-slow {\n  0%, 100% {\n    opacity: 1;\n  }\n  50% {\n    opacity: 0.7;\n  }\n}\n\n.animate-pulse-slow {\n  animation: pulse-slow 3s cubic-bezier(0.4, 0, 0.6, 1) infinite;\n}\n\n.animation-delay-1000 {\n  animation-delay: 1s;\n}\n</style>\n"],"mappings":"AA8FA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,QAAQ,QAAQ,KAAI;;AAE5C;;;;;;;;;;;;;;;;;;;IACA,IAAMC,KAAK,GAAGC,OAAA;;IAWd;IACA,IAAMC,IAAI,GAAGC,MAAA;;IAEb;IACA,IAAMC,aAAa,GAAG,CACpB;MAAEC,KAAK,EAAE,QAAQ;MAAEC,KAAK,EAAE;IAAK,CAAC,EAChC;MAAED,KAAK,EAAE,QAAQ;MAAEC,KAAK,EAAE;IAAK,CAAC,EAChC;MAAED,KAAK,EAAE,QAAQ;MAAEC,KAAK,EAAE;IAAK,CAAC,EAChC;MAAED,KAAK,EAAE,QAAQ;MAAEC,KAAK,EAAE;IAAK,EACjC;;IAEA;IACA,IAAMC,YAAY,GAAG,CACnB;MAAEF,KAAK,EAAE,QAAQ;MAAEC,KAAK,EAAE;IAAK,CAAC,EAChC;MAAED,KAAK,EAAE,QAAQ;MAAEC,KAAK,EAAE;IAAK,CAAC,EAChC;MAAED,KAAK,EAAE,UAAU;MAAEC,KAAK,EAAE;IAAK,CAAC,EAClC;MAAED,KAAK,EAAE,QAAQ;MAAEC,KAAK,EAAE;IAAK,CAAC,EAChC;MAAED,KAAK,EAAE,SAAS;MAAEC,KAAK,EAAE;IAAK,CAAC,EACjC;MAAED,KAAK,EAAE,YAAY;MAAEC,KAAK,EAAE;IAAK,EACrC;;IAEA;IACA,SAASE,cAAcA,CAACH,KAAK,EAAE;MAC7B,IAAMI,MAAM,GAAGL,aAAa,CAACM,IAAI,CAAC,UAAAC,MAAM;QAAA,OAAIA,MAAM,CAACN,KAAK,KAAKA,KAAK;MAAA;MAClE,OAAOI,MAAM,GAAGA,MAAM,CAACH,KAAK,GAAGD,KAAI;IACrC;;IAEA;IACA,SAASO,aAAaA,CAACP,KAAK,EAAE;MAC5B,IAAMQ,KAAK,GAAGN,YAAY,CAACG,IAAI,CAAC,UAAAC,MAAM;QAAA,OAAIA,MAAM,CAACN,KAAK,KAAKA,KAAK;MAAA;MAChE,OAAOQ,KAAK,GAAGA,KAAK,CAACP,KAAK,GAAGD,KAAI;IACnC;;IAEA;IACA,SAASS,eAAeA,CAACC,QAAQ,EAAE;MACjC,IAAMC,OAAO,GAAG;QACd,KAAK,EAAE,QAAQ;QACf,QAAQ,EAAE,OAAO;QACjB,MAAM,EAAE,aAAa;QACrB,WAAW,EAAE,SAAS;QACtB,WAAW,EAAE,MAAM;QACnB,OAAO,EAAE,UAAU;QACnB,WAAW,EAAE,QAAQ;QACrB,SAAS,EAAE;MACb;MACA,OAAOA,OAAO,CAACD,QAAQ,CAAC,IAAIC,OAAO,CAAC,SAAS;IAC/C;;IAEA;IACA,SAASC,cAAcA,CAAA,EAAG;MACxBf,IAAI,CAAC,YAAY;IACnB","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}