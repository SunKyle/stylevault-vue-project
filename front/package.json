{
  "name": "stylevault",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "dev": "vue-cli-service serve",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:inspiration": "vitest run test-inspiration-comprehensive.js",
    "test:e2e": "vitest run test-inspiration-e2e.js",
    "test:performance": "vitest run --reporter=verbose test-inspiration-comprehensive.js",
    "test:debug": "vitest run --reporter=verbose --no-coverage"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.1.1",
    "@tanstack/vue-virtual": "^3.13.12",
    "axios": "^1.11.0",
    "core-js": "^3.8.3",
    "pinia": "^3.0.3",
    "vue": "^3.5.19",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@pinia/testing": "^1.0.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-vue": "^8.0.3",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.4.17",
    "vite": "^7.1.3",
    "vitest": "^3.2.4"
  },
  "main": "tailwind.config.js",
  "keywords": [],
  "license": "ISC",
  "description": ""
}
